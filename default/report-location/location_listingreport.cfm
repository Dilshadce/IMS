                <option value="#i#" <cfif #sitemdesp# eq #getgeneral.ddlitem#>selected</cfif>>#sitemdesp#</option>
                </cfloop>
				</cfoutput>
				</select>
                <cfelse>
            
				<select name="productto">
					<option value="">Choose a Product</option>
					<cfoutput query="getitem">
