<cfquery name="getgeneral" datasource="#dts#">
	select 
	date_format(lastaccyear,'%d-%m-%Y') as lastaccyear,filterall,lCATEGORY,lGROUP,lSIZE,lMATERIAL,lMODEL,lRATING,lAGENT,lDRIVER,lLOCATION,singlelocation,filteritemreport,ddlitem,lsize,lmaterial
	from gsetup;
</cfquery>

