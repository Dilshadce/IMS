                                    RIGHT JOIN userdefinedmenu AS udm ON m.menu_id = udm.menu_id
                                    WHERE m.menu_parent_id =#getLevel1Menu.menu_id# OR udm.menu_parent_id=#getLevel1Menu.menu_id#
